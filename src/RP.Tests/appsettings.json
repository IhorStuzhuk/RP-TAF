{
  "WebConfig": {
    "UrlClient": "https://reportportal.epam.com"
  },

  "WebBrowserConfig": {
    "BrowserName": "{BROWSER_NAME}",
    "IsRemote": false,
    "ImplicitWaitTimeOutSec": 10,
    "MakeScreenshot": false,
    "GridHubUrl": "http://localhost:4444/wd/hub"
  },

  "UserConfig": {
    "User": "{USER_LOGIN}",
    "Password": "{USER_PASSWORD}"
  },

  "ApiConfig": {
    "Host": "https://reportportal.epam.com/api/v1",
    "ProjectName": "{USER_PROJECT}",
    "BearerToken": "{API_KEY}"
  },

  "JiraConfig": {
    "URL": "https://istuzhuk.atlassian.net",
    "User": "{JIRA_USER}",
    "Token": "{JIRA_TOKEN}"
  },

  "MSTeamsWebHookUrl": "{MS_TEAMS_WEBHOOK_URL}",

  "Logger": "Console"
}