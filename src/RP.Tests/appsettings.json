{
    "WebConfig": {
      "UrlClient": "https://reportportal.epam.com"
    },

    "WebBrowserConfig": {
      "BrowserName": "${{github.event.inputs.browserName}}",
      "IsRemote": false,
      "ImplicitWaitTimeOutSec": 20,
      "MakeScreenshot": false,
      "GridHubUrl": "http://localhost:4444/wd/hub"
    },

    "UserConfig": {
      "User": "${{secrets.USER_LOGIN}}",
      "Password": "${{secrets.USER_PASSWORD}}"
    },

    "ApiConfig": {
      "Host": "https://reportportal.epam.com/api/v1",
      "ProjectName": "${{secrets.USER_PROJECT}}",
      "BearerToken": "${{secrets.API_KEY}}"
    },

    "Logger": "Console"
}