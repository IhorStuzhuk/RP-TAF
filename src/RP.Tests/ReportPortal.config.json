{
  "$schema": "https://raw.githubusercontent.com/reportportal/agent-net-nunit/master/src/ReportPortal.NUnitExtension/ReportPortal.config.schema",
  "enabled": true,
  "server": {
    "url": "https://reportportal.epam.com",
    "project": "${{secrets.REPORTING_PROJECT}}",
    "authentication": {
      "uuid": "${{secrets.REPORTING_AUTH}}"
    }
  },
  "launch": {
    "name": "RP tests",
    "description": "",
    "debugMode": true,
    "attributes": [ "RPTests" ]
  },
  "rootNamespaces": [ "RP.Tests.dll", "RP", "Tests" ]
}
